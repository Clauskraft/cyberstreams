# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# PostgreSQL Configuration (optional)
# DATABASE_URL=postgresql://user:password@localhost:5432/cyberstreams
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_DB=cyberstreams
# POSTGRES_USER=cyberstreams
# POSTGRES_PASSWORD=your-secure-password

# MISP Integration (optional)
# MISP_BASE_URL=https://your-misp-instance.com
# MISP_API_KEY=your-misp-api-key-here
# MISP_VERIFY_TLS=true

# OpenCTI Integration (optional)
# OPENCTI_API_URL=https://your-opencti-instance.com:8080
# OPENCTI_PUBLIC_URL=https://your-opencti-instance.com
# OPENCTI_TOKEN=your-opencti-token-here

# Vector Database Configuration (optional)
# VECTOR_DB_URL=http://localhost:6333
# VECTOR_DB_API_KEY=your-vector-db-token-here

# Intel Scraper Configuration (optional)
# AUTO_START_INTEL_SCRAPER=false
# AUTO_SEED_SOURCES=true
# AUTHORIZED_SOURCES_TABLE=sources
# INGESTION_LOG_LEVEL=info
