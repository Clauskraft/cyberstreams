{
  "name": "cyberstreams",
  "version": "2.0.0",
  "description": "Advanced Dark Web Threat Intelligence Platform",
  "type": "module",
  "scripts": {
    "seed:keywords": "BASE_URL=http://localhost:3001 node scripts/add-keywords.js",
    "seed:dpa": "node scripts/add-dpa-sources.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server.js",
    "start": "node server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "smoketest": "node smoketest.js",
    "smoketest:frontend": "node frontend-smoketest.js",
    "smoketest:integration": "node integration-smoketest.js",
    "smoketest:mcp": "node mcp-smoketest.js",
    "smoketest:quick": "node quick-smoketest.js",
    "smoketest:all": "node run-smoketest.js",
    "userflows:run": "node scripts/user-flows.js",
    "smoketest:online": "node online-smoketest.js",
    "smoketest:online:production": "ONLINE_BASE_URL=https://cyberstreams.dk node online-smoketest.js",
    "smoketest:online:staging": "ONLINE_BASE_URL=https://staging.cyberstreams.dk node online-smoketest.js",
    "smoketest:online:both": "node online-smoketest.js",
    "monitor:online": "node monitor-online.js",
    "setup:mcp": "node setup-mcp.js",
    "mcp:start": "node mcp-servers/server-manager.js",
    "mcp:core": "node mcp-servers/core-server.js",
    "mcp:threat-intel": "node mcp-servers/threat-intel-server.js",
    "mcp:wifi-analysis": "node mcp-servers/wifi-analysis-server.js",
    "mcp:test": "node mcp-smoketest.js",
    "mcp:integration-test": "node mcp-integration-test.js",
    "security:test": "node mcp-servers/security/run-security-tests.js",
    "security:monitor": "node mcp-servers/security/security-monitor.js",
    "security:config": "node mcp-servers/security/security-config.js",
    "monitoring:start": "node mcp-servers/monitoring/monitoring-integration.js",
    "monitoring:system": "node mcp-servers/monitoring/system-monitor.js",
    "monitoring:performance": "node mcp-servers/monitoring/performance-monitor.js",
    "monitoring:alerts": "node mcp-servers/monitoring/alerting-system.js",
    "prepare": "husky",
    "lint:commit": "commitlint --edit $GIT_PARAMS",
    "ingest": "node --loader ts-node/esm scripts/cron/ingest.ts",
    "migrate:sources": "node --loader ts-node/esm scripts/migrate-authorized-sources.ts",
    "railway:build": "npm run build",
    "railway:start": "node server.js"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "mcp-framework": "^0.2.15",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.5",
    "pino": "^9.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rss-parser": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-istanbul": "^4.0.2",
    "autoprefixer": "^10.4.14",
    "husky": "^9.1.6",
    "jsdom": "^24.0.0",
    "node-cron": "^3.0.3",
    "pino-pretty": "^11.0.0",
    "playwright": "^1.56.0",
    "postcss": "^8.4.27",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.3.3",
    "terser": "^5.44.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^1.5.2"
  }
}
