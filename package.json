{
  "name": "cyberstreams-enhanced",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon server.js",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "migrate": "node scripts/migrate.mjs",
    "brand:brandA": "cross-env VITE_BRAND=brandA vite",
    "brand:brandB": "cross-env VITE_BRAND=brandB vite",
    "brand:brandC": "cross-env VITE_BRAND=brandC vite"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.546.0",
    "openai": "^4.20.0",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "puppeteer": "^19.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.0",
    "rss-parser": "^3.13.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.15",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
